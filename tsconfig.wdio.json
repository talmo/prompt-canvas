{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": false,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "declaration": false,
    "outDir": "./out/e2e",
    "rootDir": ".",
    "types": [
      "node",
      "mocha"
    ]
  },
  "include": ["e2e/**/*.ts", "wdio.conf.ts"],
  "exclude": ["node_modules"]
}
